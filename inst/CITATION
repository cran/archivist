citHeader("To cite archivist in publications use:")

citEntry(entry = "Manual",
  title        = "{archivist}: An {R} Package for Managing, Recording and Restoring Data Analysis Results",
  author       = personList(as.person("Przemys{\\l}aw Biecek"),
                   as.person("Marcin Kosinski")),
  year         = "2015",
  url          = "https://github.com/pbiecek/archivist",

  textVersion  =
  paste("Przemyslaw Biecek, Marcin Kosinski (2015).",
        "archivist: An R Package for Managing, Recording and Restoring Data Analysis Results",
        "URL https://github.com/pbiecek/archivist")
)

